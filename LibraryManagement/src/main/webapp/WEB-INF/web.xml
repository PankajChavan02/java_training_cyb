<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Demo</servlet-name>
  	<display-name>Demo</display-name>
  	<description></description>
  	<servlet-class>com.cybage.library.Demo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddBookServlet</servlet-name>
  	<display-name>AddBookServlet</display-name>
  	<description></description>
  	<servlet-class>com.cybage.library.main.AddBookServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RemoveBookById</servlet-name>
  	<display-name>RemoveBookById</display-name>
  	<description></description>
  	<servlet-class>com.cybage.library.main.RemoveBookById</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllBooks</servlet-name>
  	<display-name>AllBooks</display-name>
  	<description></description>
  	<servlet-class>com.cybage.library.main.AllBooks</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Demo</servlet-name>
  	<url-pattern>/Demo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddBookServlet</servlet-name>
  	<url-pattern>/AddBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RemoveBookById</servlet-name>
  	<url-pattern>/RemoveBookById</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllBooks</servlet-name>
  	<url-pattern>/AllBooks</url-pattern>
  </servlet-mapping>
</web-app>
