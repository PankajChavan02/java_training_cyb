<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.cybage.servlet.RequestListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<display-name>MyFirstServlet</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.MyFirstServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InitParam</servlet-name>
  	<display-name>InitParam</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.InitParam</servlet-class>
  	<init-param>
  		<param-name>username</param-name>
  		<param-value>Pankaj</param-value>
  		<description></description>
  	</init-param>
  	<init-param>
  		<param-name>password</param-name>
  		<param-value>admin</param-value>
  		<description></description>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>DemoParam</servlet-name>
  	<display-name>DemoParam</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.DemoParam</servlet-class>
  	<init-param>
  		<param-name>username</param-name>
  		<param-value>Pankaj</param-value>
  		<description></description>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>LoginForm</servlet-name>
  	<display-name>LoginForm</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.LoginForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginForm1</servlet-name>
  	<display-name>LoginForm1</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.LoginForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrationForm</servlet-name>
  	<display-name>RegistrationForm</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.RegistrationForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletContext</servlet-name>
  	<display-name>ServletContext</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.ServletContext</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Welcome</servlet-name>
  	<display-name>Welcome</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.Welcome</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LandingPage</servlet-name>
  	<display-name>LandingPage</display-name>
  	<description></description>
  	<servlet-class>com.cybage.servlet.LandingPage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<url-pattern>/MyFirstServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InitParam</servlet-name>
  	<url-pattern>/InitParam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DemoParam</servlet-name>
  	<url-pattern>/DemoParam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginForm</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
  	<servlet-name>RegistrationForm</servlet-name>
  	<url-pattern>/RegistrationForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletContext</servlet-name>
  	<url-pattern>/ServletContext</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Welcome</servlet-name>
  	<url-pattern>/Welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LandingPage</servlet-name>
  	<url-pattern>/Land</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>
